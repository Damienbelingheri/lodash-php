{
    "name": "lodash-php/lodash-php",
    "description": "A port of Lodash to PHP",
    "keywords": [
        "lodash",
        "utility",
        "utilities",
        "php-library",
        "utility-library",
        "php-utility"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Damien Belingheri",
            "email": "belingheridamien@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ]
    },
    "require": {
        "php": ">=8.0",
        "sebastian/comparator": "^1.2 | ^2.0 | ^2.1 | ^3.0 | ^4.0",
        "symfony/property-access": "^2.7 | ^3.0 | ^4.0 | ^5.0| 5.4"
    },
    "require-dev": {
        "phpdocumentor/reflection-docblock": "^5.3",
        "bamarni/composer-bin-plugin": "^1.5",
        "phpstan/phpdoc-parser": "^1.4"
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ]
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    }
}